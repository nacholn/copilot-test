# Supabase Configuration
NEXT_PUBLIC_SUPABASE_URL=your-supabase-url
NEXT_PUBLIC_SUPABASE_ANON_KEY=your-supabase-anon-key

# API Configuration
NEXT_PUBLIC_API_URL=http://localhost:3001

# VAPID Public Key for Web Push Notifications
# This is the same public key generated on the backend
# Generate keys on backend with: node -e "const webPush = require('web-push'); const vapidKeys = webPush.generateVAPIDKeys(); console.log('Public Key:', vapidKeys.publicKey); console.log('Private Key:', vapidKeys.privateKey);"
NEXT_PUBLIC_VAPID_PUBLIC_KEY=your-vapid-public-key

# Disable Next.js Telemetry
NEXT_TELEMETRY_DISABLED=1