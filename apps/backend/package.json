{
  "name": "backend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "npx tsx server.ts",
    "build": "next build",
    "start": "NODE_ENV=production npx tsx server.ts",
    "lint": "next lint",
    "clean": "rm -rf .next",
    "migrate:up": "node-pg-migrate up --migrations-file-extension=mjs",
    "migrate:down": "node-pg-migrate down --migrations-file-extension=mjs",
    "migrate:create": "node-pg-migrate create --migrations-file-extension=mjs",
    "test": "vitest run",
    "test:unit": "vitest run src/__tests__/unit",
    "test:integration": "vitest run src/__tests__/integration",
    "test:e2e": "vitest run src/__tests__/e2e",
    "test:watch": "vitest"
  },
  "dependencies": {
    "@cyclists/config": "*",
    "@supabase/supabase-js": "^2.39.0",
    "axios": "^1.13.2",
    "cloudinary": "^2.8.0",
    "dotenv": "^17.2.3",
    "form-data": "^4.0.5",
    "multer": "^2.0.2",
    "next": "^14.0.4",
    "node-fetch": "^3.3.2",
    "node-pg-migrate": "^8.0.3",
    "pg": "^8.11.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "resend": "^6.5.2",
    "socket.io": "^4.8.1"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@types/multer": "^2.0.0",
    "@types/node": "^20.10.0",
    "@types/pg": "^8.10.9",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@vitest/ui": "^4.0.15",
    "eslint": "^8.55.0",
    "eslint-config-next": "^14.0.4",
    "happy-dom": "^20.0.11",
    "tsx": "^4.21.0",
    "typescript": "^5.3.0",
    "vitest": "^4.0.15"
  }
}
